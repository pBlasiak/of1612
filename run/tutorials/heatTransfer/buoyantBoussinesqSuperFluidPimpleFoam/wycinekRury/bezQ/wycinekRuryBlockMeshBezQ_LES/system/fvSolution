potentialFlow
{
    nNonOrthogonalCorrectors 1;
}
PIMPLE
{
    nCorrectors         1;
	nOuterCorrctors     200;
    nNonOrthogonalCorrectors 1;
	momentumPredictor	on;

    pRefCell 0; 
	pRefValue 0; 
}

SIMPLE
{
    nNonOrthogonalCorrectors 1;
    residualControl
    {
        p               1e-6;
        T               1e-6;
        U               1e-6;
        "(k|epsilon|omega)"   1e-6;
    }
}

