FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      p;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -1 -2 0 0 0 0];

internalField   uniform 1E6;

boundaryField
{
     upDown
    {
	type	empty;
    }

   right
    {
	type            calculated;//fixedFluxPressure;
	value		$internalField;
    }

   left
    {
	type            calculated;//fixedFluxPressure;
	value		$internalField;
    }
   
   frontAndBack 
   {
	type	empty; 
   }

}


